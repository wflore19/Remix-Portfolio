{
	"name": "my-portfolio",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "remix vite:build",
		"dev": "remix vite:dev",
		"start": "remix-serve ./build/server/index.js",
		"lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
		"typecheck": "tsc",
		"drizzle:pull": "npx drizzle-kit generate:pg --out ./app/drizzle/migrations --schema ./app/drizzle/schema.server.ts",
		"drizzle:push": "npx drizzle-kit push",
		"drizzle:seed": "tsx ./app/drizzle/seed.ts",
		"drizzle:reset": "tsx ./app/drizzle/reset.ts",
		"drizzle:studio": "drizzle-kit studio --port=3000"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.705.0",
		"@aws-sdk/lib-storage": "^3.705.0",
		"@radix-ui/themes": "^3.1.6",
		"@remix-run/node": "^2.15.0",
		"@remix-run/react": "^2.15.0",
		"@remix-run/serve": "^2.15.0",
		"@remixicon/react": "^4.5.0",
		"dotenv": "^16.4.5",
		"drizzle-orm": "^0.36.4",
		"drizzle-seed": "^0.1.2",
		"googleapis": "^144.0.0",
		"isbot": "^4.1.0",
		"pg": "^8.13.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@remix-run/dev": "^2.15.0",
		"@types/pg": "^8.11.10",
		"@types/react": "^18.2.20",
		"@types/react-dom": "^18.2.7",
		"@typescript-eslint/eslint-plugin": "^6.7.4",
		"@typescript-eslint/parser": "^6.7.4",
		"autoprefixer": "^10.4.19",
		"drizzle-kit": "^0.28.1",
		"eslint": "^8.38.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"postcss": "^8.4.49",
		"tsx": "^4.19.2",
		"typescript": "^5.1.6",
		"vite": "^5.1.0",
		"vite-tsconfig-paths": "^4.2.1"
	},
	"engines": {
		"node": ">=20.0.0"
	}
}
